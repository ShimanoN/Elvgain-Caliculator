{
  "name": "elvgain-caliculator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"js/**/*.js\" \"*.html\" \"css/**/*.css\"",
    "format:check": "prettier --check \"js/**/*.js\" \"*.html\" \"css/**/*.css\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShimanoN/Elvgain-Caliculator.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ShimanoN/Elvgain-Caliculator/issues"
  },
  "homepage": "https://github.com/ShimanoN/Elvgain-Caliculator#readme",
  "lint-staged": {
    "js/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "e2e/**/*.js": [
      "prettier --write"
    ],
    "*.{html,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@vitest/ui": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "fake-indexeddb": "^6.2.5",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "vitest": "^4.0.18"
  }
}
